        "row_A6feW8": {
          "type": "row",
          "settings": {
            "image": "shopify://shop_images/250811_ren_web_photo_01.png",
            "image_sp": "shopify://shop_images/250811_ren_web_photo_02.png",
            "image_and_text_gap": 5,
            "caption": "",
            "heading": "Roots",
            "heading_color": "rgba(255,255,255,1)",
            "heading_border_color": "rgba(255,255,255,0.7)",
            "heading_fontweight": 100,
            "heading_size_desktop": 24,
